// 메트릭스 조회
function para_matrix() {
	var fmDate = ds_selectData.getColumn(0,0);
	var toDate = ds_selectData.getColumn(0,1);
	var reportType = ds_selectData.getColumn(0,2);
	var categorySeq = ds_selectData.getColumn(0,3);
	var codeGB = ds_selectData.getColumn(0,4);
	var verSeq = ds_selectData.getColumn(0,5);
	
	for(var a = (ds_result.rowcount); a > 0; a--){
		Tab_item.tab_matrix.grid_matrix.deleteContentsCol(a);
	}

	// 개정버전에 따라 조회날짜 제어
	var selectFmDate = "";
	var selectToDate = "";
	var versionStartDate = Tab_item.tab_version.grd_output.getCellValue(Tab_item.tab_version.grd_output.currentrow,1);
	versionStartDate = versionStartDate.substring(0,6);
	
	if( versionStartDate > fmDate) {
		selectFmDate = versionStartDate;
	} else {
		selectFmDate = fmDate;
	}
	
	for(var b= (ds_version.rowcount-1); b >= 0; b--) {
		var nextDate = ds_version.getColumn(b,"START_DATE");
		nextDate = nextDate.substring(0,6);
		// 다음버전 개정일 한달전 날짜 계산
		if(nextDate > selectFmDate) {
			nextDateYY = nextDate.substring(0,4);
			nextDateMM = nextDate.substring(4,6);
			if(nextDateMM == "01") {
				nextDateYY = new String(parseInt(nextDateYY) -1);
				nextDateMM = "12";
			} else {
				nextDateMM = new String(parseInt(nextDateMM)-1);
				if(nextDateMM.length < 2) nextDateMM = "0"+nextDateMM;
			}
			selectToDate = nextDateYY+nextDateMM;
			break;
		} else {
			selectToDate = toDate;
		}
	}
	//trace(selectFmDate);
	//trace(selectToDate);
	
	// 조회기간 표시 수정
	var selectDate = selectFmDate.substring(0,4) + "." + selectFmDate.substring(4,6) + " ~ " + selectToDate.substring(0,4) + "." + selectToDate.substring(4,6);
	Tab_item.tab_matrix.Div_condition.Static_selectDate.text = selectDate;
	
	// 조회기간 값 변경
	ds_selectData.setColumn(0,0,selectFmDate);
	ds_selectData.setColumn(0,1,selectToDate);
	
	transaction("tra_itemCatagoryMatrix"
		,"siteURL::risk.do?action=itemCatagoryMatrix"
		,""
		,"ds_matrix=ds_matrix ds_result=ds_result"
		,"itemCategorySeq='" + categorySeq + "' " + "fmDate='" + selectFmDate + "' toDate='" + selectToDate + "' verDate='" + versionStartDate 
			+ "' reportType='"+reportType + "' codeGB='"+codeGB+"' verSeq="+verSeq
		,"fn_callback");
}

// grid_matrix fromat편집
function fn_matrix_grid() {
	Tab_item.tab_matrix.grid_matrix_list.visible=false;
	for(var a = 0; a < ds_result.rowcount; a++)
	{
		Tab_item.tab_matrix.grid_matrix.appendContentsCol();
		var colorColName = new String(ds_result.getColumn(a,2))+"COLOR";
		
		var colText = ds_result.getColumn(a,0)+"."+ds_result.getColumn(a,1);
		var colBind = ds_result.getColumn(a,1);
		
		Tab_item.tab_matrix.grid_matrix.setCellProperty("Head", a+1, "text", colText);
		Tab_item.tab_matrix.grid_matrix.setCellProperty("Head", a+1, "font", "굴림체,9,bold;");
		Tab_item.tab_matrix.grid_matrix.setCellProperty("Head", a+1, "selectfont", "굴림체,9,bold;");
		Tab_item.tab_matrix.grid_matrix.setCellProperty("Body", a+1, "text", "bind:"+colBind);
		Tab_item.tab_matrix.grid_matrix.setCellProperty("Body", a+1, "background", "bind:"+colorColName);
		Tab_item.tab_matrix.grid_matrix.setCellProperty("Body", a+1, "background2", "bind:"+colorColName);
	}
	Tab_item.tab_matrix.grid_matrix.setBandProperty("Head","style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;" );
	Tab_item.tab_matrix.grid_matrix.setBandProperty("Body","style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;" );
	//trace(Tab_item.tab_matrix.grid_matrix.getCurFormatString());
	Tab_item.tabindex = 2;
}

// matrix 건수 상세조회
function Tab_item_tab_matrix_grid_matrix_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.col > 0) {
		if(obj.getCellValue(e.row, e.col) > 0) {
			Tab_item.tab_matrix.grid_matrix_list.visible=true;
			para_matrix_list(e.col, e.row);
		} else {
			Tab_item.tab_matrix.grid_matrix_list.visible=false;
		}
	}
}

// 메트릭스 조회
function para_matrix_list(colNo, rowNo) {
	var fmDate = ds_selectData.getColumn(0,0);
	var toDate = ds_selectData.getColumn(0,1);
	var reportType = ds_selectData.getColumn(0,2);
	var categorySeq = ds_selectData.getColumn(0,3);
	var codeGB = ds_selectData.getColumn(0,4);
	var verSeq = ds_selectData.getColumn(0,5);
	var resultSeq = ds_result.getColumn(colNo-1,2);
	var possibilitySeq = ds_matrix.getColumn(rowNo,0);

	transaction("tra_matrixList"
	,"siteURL::risk.do?action=matrixList"
	,""
	,"ds_matrix_list=ds_matrix_list"
	,"itemCategorySeq='" + categorySeq + "' " + "fmDate='" + fmDate + "' toDate='" + toDate 
		+ "' reportType='"+reportType + "' codeGB='"+codeGB+"' verSeq="+verSeq + " resultSeq="+resultSeq + " possibilitySeq="+possibilitySeq
	,"fn_callback");

}



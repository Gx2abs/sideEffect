/*************************************************엑셀,출력 시작***************************************************************/
// 엑셀파일
function btn_excel_onclick(obj:Button,  e:ClickEventInfo)
{
	var nCnt = ExtObj0.clear();
	var vTabGB = Tab_item.tabindex;

	ExtObj0.exporttype = ExportTypes.EXCEL;
	
	var sSheetName = "ExcelDown";
	var sFileName = "조회코드.xlsx";
	
	ExtObj0.activepagename = sSheetName;	
	ExtObj0.exportfilename = sFileName;
	ExtObj0.exportopenmode = "existopen";

	if(vTabGB == 0) {
		var nRet = ExtObj0.addExportItem(ExportItemTypes.GRID, Tab_item.tab_list.grd_output, sSheetName+"!A1" );
	} else if(vTabGB == 1) {
		 //엑셀용grid에 set
		 ds_item_view.setColumn(0,0,Tab_item.tab_version.Div_condition.Static_item_name.text);
		 ds_item_view.setColumn(0,1,Tab_item.tab_version.Div_condition.Static_class_no.text);
		 ds_item_view.setColumn(0,2,Tab_item.tab_version.Div_condition.Static_selectDate.text);
		 ds_item_view.setColumn(0,3,Tab_item.tab_version.Div_condition.Static_codeType.text);
		 ds_item_view.setColumn(0,4,Tab_item.tab_version.Div_condition.Static_codeCnt.text);
		 
		var nRet = ExtObj0.addExportItem(ExportItemTypes.GRID, Tab_item.tab_version.grid_item_view, sSheetName+"!B1" );
		 nRet = ExtObj0.addExportItem(ExportItemTypes.GRID, Tab_item.tab_version.grd_output, sSheetName+"!A7" );
	} else if(vTabGB == 2) {
		 //엑셀용grid에 set
		 ds_item_view.setColumn(0,0,Tab_item.tab_matrix.Div_condition.Static_item_name.text);
		 ds_item_view.setColumn(0,1,Tab_item.tab_matrix.Div_condition.Static_class_no.text);
		 ds_item_view.setColumn(0,2,Tab_item.tab_matrix.Div_condition.Static_selectDate.text);
		 ds_item_view.setColumn(0,3,Tab_item.tab_matrix.Div_condition.Static_codeType.text);
		 ds_item_view.setColumn(0,4,Tab_item.tab_matrix.Div_condition.Static_codeCnt.text);
		 
		 // selecttype 속성이 cell 이면 head가 출력되지 않음
		 Tab_item.tab_matrix.grid_matrix.selecttype = "row";
		 
		 var nRet = ExtObj0.addExportItem(ExportItemTypes.GRID, Tab_item.tab_matrix.grid_item_view, sSheetName+"!B1" );
		 nRet = ExtObj0.addExportItem(ExportItemTypes.GRID, Tab_item.tab_matrix.grid_matrix, sSheetName+"!A7" );
		 var dsRowCnt = ds_matrix.rowcount + 7+3;
		 nRet = ExtObj0.addExportItem(ExportItemTypes.GRID, Tab_item.tab_matrix.grid_matrix_list, sSheetName+"!A"+dsRowCnt );
	}
	
	ExtObj0.export();
	Tab_item.tab_matrix.grid_matrix.selecttype =  "cell";
}


// 출력
function btn_print_onclick(obj:Button,  e:ClickEventInfo)
{
	var vTabGB = Tab_item.tabindex;
	if (vTabGB == 0) {
		Tab_item.tab_list.print(false,"top");
	} else if (vTabGB == 1) {
		Tab_item.tab_version.print(false,"top");
	} else if (vTabGB == 2) {
		Tab_item.tab_matrix.print(false,"top");
	}
}


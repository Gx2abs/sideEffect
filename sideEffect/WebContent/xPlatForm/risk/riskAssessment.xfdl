// 조회 결과 
function fn_callback(trID, errCD, errMSG) 
{	
	if (trID == "tra_retrieve" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} 
	} else if (trID == "tra_reportType" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			Tab_item.tab_list.Div_condition.Combo_reportType.index = "0";
		}
	} else if (trID == "tra_productCode" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			Tab_item.tab_list.Div_condition.Combo_product.index = "0";
		}
	} else if (trID == "tra_itemLikelihoodList" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			// 검색조건 총 건수 표시
			Tab_item.tab_list.Static_total.text = "* 총 "+errMSG+"건."
			totalCnt = Number(errMSG);
			ds_category.keystring  = "S:-GET_SUM";
			// 더보기 버튼 표시체크
			var dsCnt = ds_category.rowcount;
			Tab_item.tab_list.Static_currentTotal.text = "- ("+dsCnt+")건 조회"
			if ( dsCnt >= totalCnt) {
				Tab_item.tab_list.btn_more.visible = false;
			} else {
				Tab_item.tab_list.btn_more.visible = true;
			}
			
			Tab_item.tab_version.enable = false;
			Tab_item.tab_matrix.enable = false;
		}
	} else if (trID == "tra_itemLikelihoodList_more" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			
			ds_category.appendData(ds_category_more);
			ds_category.keystring  = "S:-GET_SUM";
			// 더보기 버튼 표시체크
			var dsCnt = ds_category.rowcount;
			Tab_item.tab_list.Static_currentTotal.text = "- ("+dsCnt+")건 조회"
			if ( dsCnt >= totalCnt) {
				Tab_item.tab_list.btn_more.visible = false;
			} else {
				Tab_item.tab_list.btn_more.visible = true;
			}
		}
	} else if (trID == "tra_itemCatagoryGroup") {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			Tab_item.tab_version.enable = true;
			Tab_item.tabindex = 1;
		}
	} else if (trID == "tra_itemCatagoryMatrix") {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			Tab_item.tab_matrix.enable = true;
			fn_matrix_grid();
		}
	} else if (trID == "tra_matrixList") {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			
		}
	}
}

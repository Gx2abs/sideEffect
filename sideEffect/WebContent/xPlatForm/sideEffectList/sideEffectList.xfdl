	    ds_sc.keystring = "S:+ORDERID+SC"; 
	    
	} else {	
		// 초기날짜셋팅
		var year = Today();
		Div_condition.Calendar_fm_y.value =  (year.substring(0,4)-1) + '0101';
		Div_condition.Calendar_fm_m.value = Div_condition.Calendar_fm_y.value

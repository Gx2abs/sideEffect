// 메뉴팝업 - 선택
function PopupStepMenu_onmenuclick(obj:PopupMenu, e:MenuClickEventInfo)
{
	var curStep = ds_stepMenu.getColumn(0,"CURRENT_STEP");

	if (e.id=="L") {
		fn_popList(curStep);
	} else {
		if(curStep==1 && Div_condition2.visible==true && Div_condition2.Combo_search2.index > -1) {
			Step_Search(curStep+1);
		} else if(curStep==2 && Div_condition3.visible==true && Div_condition3.Combo_search3.index > -1) {
			Step_Search(curStep+1);
		} else {
			View_Stem(curStep);
		}
	} 
}



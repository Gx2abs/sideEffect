/*************************************************목록페이지 연결시작***************************************************************/
// 목록페이지 연결
function fn_popList(curStep)
{
	// 선택 셀 값 , 선택 셀의 코드 이름
	var vCellValue = de_select_code.getColumn(0,"STEP_ID");
	var vCellName = " * " + de_select_code.getColumn(0,"STEP_NAME");
	if (vCellValue < 0) {
		return;
	}
	// 코드구분
	var popName = new Array();
	// 코드값
	var codeArr = new Array();
	
	// 조회날짜종류
	var vReportType = Div_condition.Combo_reportType.value;
	
	// 조회날짜
	var fmDate = ds_stepMenu.getColumn(0,"FM_DATE");
	var toDate = ds_stepMenu.getColumn(0,"TO_DATE");

	// 조합통계일경우 
	if (curStep == 2) {
		popName.push(de_select_code.getColumn(1,"STEP_CODE"));
		codeArr.push(de_select_code.getColumn(1,"STEP_ID"));
		vCellName = vCellName + " | " + de_select_code.getColumn(1,"STEP_NAME")
	} else if (curStep == 3) {
		popName.push(de_select_code.getColumn(1,"STEP_CODE"));
		popName.push(de_select_code.getColumn(2,"STEP_CODE"));
		codeArr.push(de_select_code.getColumn(1,"STEP_ID"));
		codeArr.push(de_select_code.getColumn(2,"STEP_ID"));
		vCellName = vCellName + " | " + de_select_code.getColumn(1,"STEP_NAME")
		vCellName = vCellName + " | " + de_select_code.getColumn(2,"STEP_NAME")
	} 
	
	popName.push(PopupDiv_code.bindgestureset);  
	
	// ds_childCode 의 rowCount 가 0 보다 크다는건 하위레벨 코드 포함 조회를 의미
	if (ds_childCode.rowcount > 0 ) {
		var vCurCode = ds_data1.getColumn(grd_output.currentrow, "CODE");
		for(var i=0; i< ds_childCode.rowcount-1; i++ ) {
			var vParent = ds_childCode.getColumn(i, "PARENT_ID")
			if (vParent == vCurCode) {
				codeArr.push(ds_childCode.getColumn(i, "CHILD_ID"));
			}
		}
	} else {
		codeArr.push(vCellValue);
	}
	
	// 보고자유형
	var reporterTypeArr = new Array();
	if (ds_reporterTypes.rowcount > 0) {
		for(var i=0; i< ds_reporterTypes.rowcount; i++) {
			if(ds_reporterTypes.getColumn(i,"CHECK_YN") == 1) {
				reporterTypeArr.push(ds_reporterTypes.getColumn(i,"ID"));
			}
		}
	}

	userNotify(1, popName);	
	userNotify(2, fmDate);
	userNotify(3, toDate);
	userNotify(4, curStep);
	userNotify(5, vReportType);
	userNotify(6, codeArr);
	userNotify(7, vCellName);
	userNotify(8, reporterTypeArr);
}



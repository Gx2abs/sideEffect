*/
function searchValueCheck(){
	var result = true;
	var searchWord = PopupDiv_code.Edit_searchWord.value;
	if(searchWord!=undefined){
		for(var k=0; k<badList.length; k++){
			if(searchWord.indexOf(badList[k]) > -1){
				alert("특수문자 및 허용하지 않는 문자열이 포함되어있습니다.");
				result = false;
				return result;
				break;
			}
		}	
	}
	
	return result;
}




/*
Level별 조회일경우에 GridTree 속성갑 조심해서 변경해야함  
선택한 Level이하의 합계를 가져와야하기때문에 ds_childCode 사용하여 개발
*/

// 당일날짜 계산
function Today()
{
	var sToday = "";
	var objDate = new Date();
	var sToday  = objDate.getFullYear().toString();
	sToday += Right("0" + (objDate.getMonth() + 1), 2);
	sToday += Right("0" + objDate.getDate(), 2);

	return sToday;
}

function Right(sOrg, nSize)
{
	if( sOrg == null )
		return "";
	if( sOrg.length < nSize )
		return sOrg;
	else
		return sOrg.substr(sOrg.length-nSize, nSize);
}

function statistics_onload(obj:Form, e:LoadEventInfo)
{
	// 초기날짜셋팅
	var year = Today();
	Div_condition.Calendar_fm_y.value =  (year.substring(0,4)-1) + '0101';
	Div_condition.Calendar_fm_m.value = Div_condition.Calendar_fm_y.value
	Div_condition.Calendar_to_y.value = (year.substring(0,4)-1) + '1231';
	Div_condition.Calendar_to_m.value = Div_condition.Calendar_to_y.value
	Div_condition.Calendar_fm_y.displaynulltext = year.substring(0,4);
	Div_condition.Calendar_to_y.displaynulltext = year.substring(0,4);
	
	// 보고종류 조회 
	transaction("tra_reportType",
		"siteURL::statistics.do?action=reportTypeList"
		,"",
		"ds_ReportType=ds_ReportType"
		,""
		,"fn_callback");
		
	// 보고자유형 조회 
	transaction("tra_reporterType",
				"siteURL::statistics.do?action=reporterType"
				,"",
				"ds_reporterTypes=ds_reporterTypes"
				,""
				,"fn_Callback");

}



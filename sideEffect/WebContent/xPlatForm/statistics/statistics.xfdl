	var IDStep2 = 0;
	var IDStep3 = 0;
	
	if(curStep == 2) {
		codeStep3 = "";
	} 
	
	// 조회
	transaction("tra_selectStep",
		"siteURL::statistics.do?action="+vAction+"Search"
		,"de_select_code=de_select_code ds_reporterTypes=ds_reporterTypes"
		,"ds_step"+curStep+"=ds_step"+curStep
		,"varfmDate="+fmDate+" vartoDate="+toDate+" varReportType="+vReportType+" curStep="+curStep
		,"fn_callback");
}




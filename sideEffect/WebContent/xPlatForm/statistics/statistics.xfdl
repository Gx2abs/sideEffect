var exMaxYEAR = "";
var exMinYEAR = "";

function fn_crossTab()
{	
	// 조합통계값 초기화
	View_Stem(0);
	
	// column 정리
	ds_data1.clearData();
	ds_data1.updatecontrol= false;
	if (exMaxYEAR != "" && exMinYEAR != "") {
		ds_data1.deleteColumn("SUM");
		for (var i = exMinYEAR; i <= exMaxYEAR; i++)
		{
			ds_data1.deleteColumn("C_" + i);
		}
	}
	
	var nAddRow	 = 0;	
	var strTmp   = "";
	var nSum     = 0;
	var sMaxYEAR = ds_data0.getMax("YEAR");	
	var sMinYEAR = ds_data0.getMin("YEAR");			

	//소팅후 구현
	//ds_data0.keystring="S:CODE+YEAR";

	for (var i = sMaxYEAR; i >= sMinYEAR; i--)
	{
		if (ds_data0.findRow("YEAR", i) > -1)
		{
			ds_data1.addColumn("C_" + i, "Int", 20);
		}	
	}
	ds_data1.addColumn("SUM", "Int", 20);		
	ds_data1.enableevent = false;
	
	for (var i = 0; i < ds_data0.rowcount; i++)
	{	
		if (ds_data0.getColumn(i,"CODE") != strTmp)
		{	
			nAddRow = ds_data1.addRow();
			ds_data1.setColumn(nAddRow, "CODE", ds_data0.getColumn(i, "CODE"));
			ds_data1.setColumn(nAddRow, "NAME", ds_data0.getColumn(i, "NAME"));						
			ds_data1.setColumn(nAddRow, "C_" + ds_data0.getColumn(i, "YEAR"), ds_data0.getColumn(i, "VALUE"));	
			ds_data1.setColumn(nAddRow, "DEPTH_LEVEL", ds_data0.getColumn(i, "DEPTH_LEVEL"));
            ds_data1.setColumn(nAddRow, "SUM" , ds_data0.getCaseSum("CODE == '" + ds_data0.getColumn(i, "CODE") + "'", "VALUE"), 2);		
		} 
		else 
		{	
			nSum = Math.round(Number(fn_Trim(ds_data1.getColumn(nAddRow, "C_" + ds_data0.getColumn(i,"YEAR")))) 
			     + Number(ds_data0.getColumn(i, "VALUE")), 2);
			ds_data1.setColumn(nAddRow, "C_" + ds_data0.getColumn(i, "YEAR"), nSum);
		}

		strTmp = ds_data0.getColumn(i, "CODE");
	}	 

	ds_data1.enableevent = true;	
	
	grd_output.enableredraw = false;
	grd_output.createFormat();
	grd_output.deleteContentsCol(0);	// grid 에서 코드삭제
	grd_output.deleteContentsCol(1);	// grid 에서 depth_level 삭제
	grd_output.setFormatColProperty(0, "size", 300);	
	grd_output.setFormatColProperty(0, "band", "left");
 	grd_output.setCellProperty("Head", 0, "text", "CODE");		
 	grd_output.setCellProperty("Head", 0, "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
    grd_output.setCellProperty("Head", grd_output.getCellCount("Head") - 1, "text", "SUM");	
    grd_output.setCellProperty("Head", grd_output.getCellCount("Head") - 1, "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
    grd_output.setFormatColProperty(grd_output.getCellCount("Head") - 1, "band", "right");	
    grd_output.setCellProperty("Body", 0, "celltype", "head");	
    grd_output.setCellProperty("Body", 0, "style", "font:굴림체,9;selectfont:굴림체,9,bold;");
    grd_output.setCellProperty("Body", 0, "treelevel", "");
    grd_output.setCellProperty("Body", 0, "displaytype", "tree");
    grd_output.setCellProperty("Body", 0, "edittype", "tree");
    grd_output.setCellProperty("Body", grd_output.getCellCount("Head") - 1, "celltype", "head");
    grd_output.setCellProperty("Body", grd_output.getCellCount("Head") - 1, "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
	grd_output.appendContentsRow("summ");
    grd_output.setCellProperty("Summ", 0, "text", "TOTAL");	
    grd_output.setCellProperty("Summ", 0, "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
			
    //합계	
	var nJ = 1;

	for (var i = sMaxYEAR; i >= sMinYEAR; i--)
	{
		if (ds_data0.findRow( "YEAR", i) > -1)
		{
            grd_output.setCellProperty( "Head", nJ  , "text",  grd_output.getCellText(-1, nJ).substr(2,4)+"."+grd_output.getCellText(-1, nJ).substr(6,2));
            grd_output.setCellProperty( "Head", nJ  , "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
            grd_output.setCellProperty( "Body", nJ  , "align", "right");
            grd_output.setCellProperty( "Body", nJ  , "style", "font:굴림체,9;selectfont:굴림체,9,bold;");
            grd_output.setCellProperty( "Summ", nJ  , "align", "right");
            grd_output.setCellProperty( "Summ", nJ  , "displaytype", "number");
            grd_output.setCellProperty( "Summ", nJ  , "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
			grd_output.setCellProperty( "Summ", nJ++, "text",  ""); // ds_data1.getSum("C_" + i)
		}	
	}
	
	grd_output.setCellProperty( "Summ", nJ  , "text",  ds_data0.getSum("VALUE"));
	grd_output.setCellProperty( "Summ", nJ  , "displaytype", "number");
	grd_output.setCellProperty( "Summ", nJ  , "align", "right");
	grd_output.setCellProperty( "Summ", nJ  , "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
	grd_output.setCellProperty( "Body", nJ  , "align", "right");
	grd_output.setCellProperty( "Body", nJ  , "style", "font:굴림체,9,bold;selectfont:굴림체,9,bold;");
	grd_output.enableredraw = true;
	//trace(grd_output.getCurFormatString());

	//합계순서로 정렬 
	ds_data1.keystring = "S:-SUM";
	ds_data1.rowposition = 0;	
	
	exMaxYEAR = sMaxYEAR;
	exMinYEAR = sMinYEAR;
}


/* 공백제거 - Script::Script_String_Trim.xfdl 참조 */
function fn_Trim(sValue)
{
    if (sValue == null) return "";
    if (new String(sValue).valueOf() == "undefined") return "";
    if (new String(sValue) == null) return "";

    var retVal = (new String(sValue)).replace(/^\s+|\s+$/g, '');
    return retVal;
}


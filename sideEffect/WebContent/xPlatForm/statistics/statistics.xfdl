var oldvReportType = "";
var oldfmYY = "";
var oldfmMM = "";
var oldtoYY = "";
var oldtoMM = "";
var oldLevel = 0;

// 코드조회
function fn_popupRetrieve(actionCode, joinTable, tra, selectGB)
{
		
	// 보고자유형 선택유무 체크
	var chk=0;
	var ds_reporterTypes_rowType = 0;
	for(var i=0; i< ds_reporterTypes.rowcount; i++ ) {
		if (ds_reporterTypes.getColumn(i,"CHECK_YN") == "1" && chk < 2 ) {
			Div_condition.Combo_reporterType.text = ds_reporterTypes.getColumn(i,"NAME");
			Div_condition.Combo_reporterType.displaynulltext = ds_reporterTypes.getColumn(i,"NAME");
			chk++;
		} 
		// ds 데이터 변경 체크
		if (ds_reporterTypes.getRowType(i) == 4 ) {
			ds_reporterTypes_rowType++;
		}
	}
	if (chk < 1) {
		alert("보고자유형을 선택하지 않았습니다. 1개이상 선택해야합니다.");
		return;
	}
	
	if (actionCode == '' || actionCode == null) {
		alert("조회항목을 선택하여 주시기 바랍니다.")
		return;
	}	
	// 보고종류 
	var vReportType = Div_condition.Combo_reportType.value;
	var fmYY = new String ( Div_condition.Calendar_fm_y.value);
	var fmMM = new String (Div_condition.Calendar_fm_m.value);
	var toYY = new String (Div_condition.Calendar_to_y.value);
	var toMM = new String (Div_condition.Calendar_to_m.value);
	fmYY = fmYY.substring(0,4);
	fmMM = fmMM.substring(4,6);
	toYY = toYY.substring(0,4);
	toMM = toMM.substring(4,6);
	
	var vlevel = 0;
	var oldActionCode = PopupDiv_code.bindgestureset;
	var row = ds_codeList.rowcount;
	vlevel = PopupDiv_code.Combo_level.value;
	//alert("(oldLevel == vlevel)=="+(oldLevel == vlevel) + " oldLevel==="+ oldLevel + " vlevel==="+ vlevel);
	if ((oldActionCode == actionCode) && (row > 0) && (oldvReportType == vReportType) && (oldfmYY == fmYY) && 
		(oldfmMM == fmMM) && (oldtoYY == toYY) && (oldtoMM == toMM) && (oldLevel == vlevel) && (ds_reporterTypes_rowType < 1)) {
		fn_popupDivPosition(vObj);
	} else {
		oldvReportType = vReportType;
		oldfmYY = fmYY;
		oldfmMM = fmMM;
		oldtoYY = toYY;
		oldtoMM = toMM;
		oldLevel = vlevel;
	
		if (vChildYN == "Y") {
			PopupDiv_code.closePopup();
			PopupDiv_code.Combo_level.visible = true;
			PopupDiv_code.Static_level.visible = true;
			// Grid 속성변경
			PopupDiv_code.grid_code.setCellProperty("Body", 0, "treestartlevel", vlevel);
			PopupDiv_code.grid_code.treeuseline = true;
			PopupDiv_code.TextArea_Parent.value = "";
			
			ds_codeList.clearData();
			//fn_popupDivPosition(vObj);
			//return;
		} else {
			PopupDiv_code.TextArea_Parent.value = "";
			PopupDiv_code.Combo_level.visible = false;
			PopupDiv_code.Static_level.visible = false;
			// Grid 속성변경
			PopupDiv_code.grid_code.setCellProperty("Body", 0, "treestartlevel", "0");
			PopupDiv_code.grid_code.treeuseline = false;
		}
		var vURL = "siteURL::statistics.do?action=reportPopupCodeList"
					+ "&reportType="+vReportType
					+ "&fmYY="+fmYY+"&fmMM="+fmMM+"&toYY="+toYY+"&toMM="+toMM+"&selectGB="+selectGB+"&level="+vlevel;
					//+ "&parentId="+parentId+ "&depthLevel="+depthLevel;
		
		ds_codeList.clearData();
		
		// 데이타 조회
		transaction(tra,
				vURL
				,"ds_reporterTypes=ds_reporterTypes"
				,"ds_codeList=ds_codeList"
				,"popupGB="+actionCode
				,"fn_pop_callback");
		PopupDiv_code.bindgestureset=actionCode;
	}
	
	// filter 초기화 후 조회
	PopupDiv_code.Edit_searchWord.value = "";
	PopupDiv_code_btn_code_search_onclick();
	
	//vChildYN = "";
}


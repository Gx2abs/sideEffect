			// 데이터 복사
			ds_codeList_all.clear();
			ds_codeList_all.appendData(ds_codeList);
			
			var vlevel = PopupDiv_code.Combo_level.value;
			//alert(vlevel);
		
			var filterStr1 =" String(NAME).indexOf('" + searchWord + "') >= 0 ";
			var filterStr2 =" String(NAME2).indexOf('" + searchWord.toUpperCase() + "') >= 0 ";
			var filterStr3 =" DEPTH_LEVEL =="+ vlevel;
			
			ds_codeList.filter("("+filterStr1+ "||" + filterStr2+")" + "&&" + filterStr3);
		
	}else{
		var filterStr1 =" String(NAME).indexOf('" + searchWord + "') >= 0 ";
		var filterStr2 =" String(NAME2).indexOf('" + searchWord.toUpperCase() + "') >= 0 ";
		
		//alert(filterStr1+ filterStr2);
		
		ds_codeList.filter(filterStr1+ "||" + filterStr2);
	}
	
	
}

// 코드선택
function PopupDiv_code_btn_code_select_onclick(obj:Button,  e:ClickEventInfo)
{
	// filter 초기화 후 조회
	PopupDiv_code.Edit_searchWord.value = "";
	PopupDiv_code_btn_code_search_onclick();

	var popName = PopupDiv_code.bindgestureset;
	
	// 선택갯수 
	var nCnt = 0;
	var nSelect = 0;
	for(var i=0; i< ds_codeList.rowcount; i++ ) {
		nSelect = parseInt(ds_codeList.getColumn(i, "CHECK_YN"));
		if (nSelect > 0 ) {
			nCnt = nCnt + 1;
		}	
	}
	
	if (nCnt < 1 ) {
		alert("1개 이상 선택해야합니다.");
		return;
	} else {
		var fmYY = new String ( Div_condition.Calendar_fm_y.value);
		var fmMM = new String (Div_condition.Calendar_fm_m.value);
		var toYY = new String (Div_condition.Calendar_to_y.value);
		var toMM = new String (Div_condition.Calendar_to_m.value);
		fmYY = fmYY.substring(0,4);
		fmMM = fmMM.substring(4,6);
		toYY = toYY.substring(0,4);
		toMM = toMM.substring(4,6);
		
		// 보고종류 
		var vReportType = Div_condition.Combo_reportType.value;
		//alert(vChildYN);

		// 부모자식코드 
		ds_childCode.deleteAll();
		if (vChildYN == "Y") {
		
		
		
		for(var i=0; i< ds_codeList.rowcount; i++ ) {
			nSelect = parseInt(ds_codeList.getColumn(i, "CHECK_YN"));
			if (nSelect > 0 ) {
				var parentId = ds_codeList.getColumn(i, "ID");
				var depthLevel = parseInt(ds_codeList.getColumn(i, "DEPTH_LEVEL"));
				ds_childCode.addRow();
				ds_childCode.setColumn(ds_childCode.rowposition, "PARENT_ID",parentId);
				ds_childCode.setColumn(ds_childCode.rowposition, "CHILD_ID",parentId);
				for(var a=i+1; a < ds_codeList.rowcount; a++ ) {
					var childId = ds_codeList.getColumn(a, "ID");
					var childDepthLevel = parseInt(ds_codeList.getColumn(a, "DEPTH_LEVEL"));	
					//trace("depthLevel==="+depthLevel + " ===childDepthLevel==="+childDepthLevel)
					if (depthLevel < childDepthLevel) {
						ds_childCode.addRow();
						ds_childCode.setColumn(ds_childCode.rowposition, "PARENT_ID",parentId);
						ds_childCode.setColumn(ds_childCode.rowposition, "CHILD_ID",childId);
					} else {
						break;
					} // 레벨비교 if종료
				} // 하위레벨 for종료
			} // 체크박스 여부 if 종료
		} // for종료
		}

		
		ds_data0.deleteAll();
		// 데이타 선택
		transaction("tra_select",
			"siteURL::statistics.do?action="+vAction
			,"ds_codeList=ds_codeList ds_childCode=ds_childCode ds_reporterTypes=ds_reporterTypes"
			,"ds_data0=ds_data0"
			,"varCodeName='"+popName+"' varfmYY="+fmYY+" varfmMM="+fmMM+" vartoYY="+toYY+" vartoMM="+toMM + " varReportType="+vReportType			
			,"fn_pop_callback");
				
	   PopupDiv_code_btn_popClose_onclick();
	}
}


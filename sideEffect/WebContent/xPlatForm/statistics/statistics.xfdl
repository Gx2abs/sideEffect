// 정렬
function PopupRMenu_onmenuclick(obj:PopupMenu, e:MenuClickEventInfo)
{
	var vId = e.id;
	var vSort;
	var vSortKey;
	
	if (vId == "D") {
		vSort = "-";
		if (vCol=="SUM") {
			vSortKey = "S:"+vSort+vCol;
		} else if(vCol=="CODE") {
			vSortKey = "S:"+vSort+"NAME";
		} else {
			vSortKey = "S:"+vSort+"C_"+vCol;
		}
		ds_data1.keystring = vSortKey;
	} else if (vId == "A") {
		vSort = "+";
		if (vCol=="SUM") {
			vSortKey = "S:"+vSort+vCol;
		} else if(vCol=="CODE") {
			vSortKey = "S:"+vSort+"NAME";
		} else {
			vSortKey = "S:"+vSort+"C_"+vCol;
		}
		ds_data1.keystring = vSortKey;
	} else if (vId == "N") {
		ds_data1.keystring = "S:-SUM";
	}
	//alert("vId==" + vId + " vCol==="+ vCol + " vSortKey==="+ vSortKey);
	//trace(ds_data1.saveXML());
}


function fn_pop_callback(trID, errCD, errMSG) 
{
	// transaction ID (=transaction 첫번째 인자), 에러코드, 에러메세지
	if(trID == "tra_select") {
		if(errCD < 0) {
			alert("전송실패."+ errMSG);
			return;
		} else {	
			PopupDiv_code_btn_popClose_onclick();
			//alert(ds_data0.rowcount + "건 조회되었습니다.");
			fn_crossTab();
			// 합계기준으로 재정렬
			ds_data1.keystring.current = "S:-SUM";
			
		}
	} else if (trID == "tra_retrieve" ) {
		if(errCD < 0) {
			PopupDiv_code_btn_popClose_onclick();
			alert("조회실패."+ errMSG);
			return;
		} else {
			
			// 영문검색 (대소문자 검색)을 위해 추가
			var _row = ds_codeList.rowcount;
			var _str;
			
			for(var i=0;i<_row;i++)
			{
				ds_codeList.addColumn("NAME2","string",900);
				_str = ds_codeList.getColumn(i,"NAME").toString().toUpperCase();
				ds_codeList.setColumn(i,"NAME2",_str);
			}
			
			if(vChildYN != "Y") {
				ds_codeList.keystring  = "S:+NAME";
			} else {
				ds_codeList.keystring  = "S:+NUM";
			}
			// 레벨별 조회 건수
			if (vChildYN == "Y") {
				
				var searchWord = PopupDiv_code.Edit_searchWord.value;
	
				if (searchWord == null) {
					searchWord = ""
				} else {
					searchWord.trim();
				}

				var vlevel = PopupDiv_code.Combo_level.value;
			
				var filterStr1 =" String(NAME).indexOf('" + searchWord + "') >= 0 ";
				var filterStr2 =" String(NAME2).indexOf('" + searchWord.toUpperCase() + "') >= 0 ";
				var filterStr3 =" DEPTH_LEVEL =="+ vlevel;
				
				ds_codeList.filter("("+filterStr1+ "||" + filterStr2+")" + "&&" + filterStr3);
				
				PopupDiv_code.Static_total.text = ds_codeList.rowcount + "건 조회되었습니다."
				
			}else{
				PopupDiv_code.Static_total.text = ds_codeList.rowcount + "건 조회되었습니다."
			}
			
			fn_popupDivPosition(vObj);
			
			//ds_codeList.keystring = "S: +NAME";
			//ds_codeList_all.keystring = "S: +NAME"; 
		}
	} 
}


function fn_callback(trID, errCD, errMSG) 
{	
	if (trID == "tra_retrieve" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			//alert(ds_data0.rowcount + "건 조회되었습니다.");
			fn_crossTab();
		}
	} else if (trID == "tra_chart" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} 
	} else if (trID == "tra_reportType" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			Div_condition.Combo_reportType.index = "0";
		}
	} else if (trID == "tra_reporterType" ) {
		if(errCD < 0) {
			alert("조회실패."+ errMSG);
			return;
		} else {
			Div_condition.Combo_reportType.index = "0";
		}
	} else if (trID == "tra_selectStep" ) {
		if(errCD < 0) {
			PopupDiv_code_btn_popClose_onclick();
			alert("검색실패."+ errMSG);
			return;
		} else {
			ds_step2.keystring  = "S:-CODE_SUM";
			ds_step3.keystring  = "S:-CODE_SUM";
		}
	}
}

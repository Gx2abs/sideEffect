// 화면표시제어
function View_Stem(stepGB) {
	ds_step_code.keystring ="S:+NAME";
	if(stepGB == "1") {
		var codeStep1 = PopupDiv_code.bindgestureset;
		if (codeStep1 == "popMedicalLevel" || codeStep1 == "popPatientLevel" || codeStep1 == "popComponentLevel" ) {
			alert("조합통계를 지원하지 않는 항목입니다.");
			return;
		}
		
		ds_step2.clear();
		Div_condition2.visible = true;
		ds_step3.clear();
		Div_condition3.visible = false;

		de_select_code.setColumn(1,"STEP_CODE", ".");
		de_select_code.setColumn(2,"STEP_CODE", ".");
		
	} else if (stepGB == "2") {
		de_select_code.setColumn(2,"STEP_CODE", ".");		
		Div_condition2.Static_search2.visible = true;
		Div_condition3.visible = true;
		ds_step3.clear();
	} else {
		// 화면표시 false 로 변경하고 데이터 초기화 
		Div_condition2.visible = false;
		Div_condition3.visible = false;
		ds_step2.clear();
		ds_step3.clear();
		de_select_code.setColumn(0,"STEP_ID",0);
		de_select_code.setColumn(1,"STEP_ID",0);
		de_select_code.setColumn(2,"STEP_ID",0);
		return;
	}

}


// 조합통계(1단계) - 메뉴팝업
function grd_output_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	
	var codeStep1 = PopupDiv_code.bindgestureset;
	if (codeStep1 == "popMedicalLevel" || codeStep1 == "popPatientLevel" || codeStep1 == "popComponentLevel" ) {
		ds_stepMenu.filter("String(ID) =='L'");
	} else {
		ds_stepMenu.filter("String(ID) IN ('L','S')");
	}
	de_select_code.setColumn(0,"STEP",1);
	de_select_code.setColumn(0,"STEP_CODE", codeStep1);
	de_select_code.setColumn(0,"STEP_ID", parseInt(ds_data1.getColumn(e.row, "CODE")));
	de_select_code.setColumn(0,"STEP_NAME", ds_data1.getColumn(e.row, "NAME"));
	
	// 조회날짜
	var fmYY = new String (Div_condition.Calendar_fm_y.value);
	var fmMM = new String (Div_condition.Calendar_fm_m.value);
	var toYY = new String (Div_condition.Calendar_to_y.value);
	var toMM = new String (Div_condition.Calendar_to_m.value);
	
	var vColText = grd_output.getCellProperty("Head",grd_output.currentcol,"text");
	if (vColText == "SUM") {
		fmYY = fmYY.substring(0,4);
		fmMM = fmMM.substring(4,6);
		toYY = toYY.substring(0,4);
		toMM = toMM.substring(4,6);
	} else if (vColText == "CODE") {
		return;
	} else {
		fmYY = vColText.substring(0,4);
		fmMM = vColText.substring(5,7);
		toYY = vColText.substring(0,4);
		toMM = vColText.substring(5,7);
	}
	ds_stepMenu.setColumn(0,"FM_DATE",fmYY+fmMM);
	ds_stepMenu.setColumn(0,"TO_DATE",toYY+toMM);
	ds_stepMenu.setColumn(0,"CURRENT_STEP",1);
	
	PopupStepMenu.trackPopup(e.screenX, e.screenY);
	
}


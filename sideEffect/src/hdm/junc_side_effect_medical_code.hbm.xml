<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	 <class table="JUNC_SIDE_EFFECT_MEDICAL_CODE" name="sideeffect.JuncSideEffectMedicalCode">
	 
		<id name="id" column="id">
			<generator class="sequence"> 
			  	<param name="sequence">JUNC_SIDE_EFFECT_MEDICAL_CODE_ID_SEQ</param>
		 	</generator>
		</id>
		 
		<property name="report_id" >
			<column name="report_id"/>
		</property>
		
		<property name="medical_id" >
			<column name="medical_id"/>
		</property>
	
	</class>
 
</hibernate-mapping>
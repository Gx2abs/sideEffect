<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class table="JUNC_SIDE_EFFECT_PATIENT_CONDITION" name="sideeffect.JuncSideEffectPatientCondition">
	 
		<id name="id" column="id">
			<generator class="sequence"> 
			  	<param name="sequence">JUNC_SIDE_EFFECT_PATIENT_CONDITION_ID_SEQ</param> 
		 	</generator>
		</id>
		 
		<property name="report_id" >
			<column name="report_id"/>
		</property>
		
		<property name="patient_id" >
			<column name="patient_id"/>
		</property>
	
	</class>
 
</hibernate-mapping>
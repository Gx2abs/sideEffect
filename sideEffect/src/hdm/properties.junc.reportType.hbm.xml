<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class table="JUNC_REPORT_TYPE_HISTORY" dynamic-update="false"
     name="properties.report.type.SimpleJuncReportType" mutable="false"
     dynamic-insert="false"  
     select-before-update="true"
     >
<id name="id" type="long"   >
 <column name="id"   />
 <generator class="sequence">
     <param name="sequence">JUNC_REPORT_TYPE_HISTORY_SEQ</param>
 </generator>
</id>
<property name="report_Type_id" type="int">
 <column name="REPORT_TYPE_ID" />
</property>
<property name="history_id" type="int">
 <column name="HISTORY_ID" />
</property>
</class>
</hibernate-mapping>

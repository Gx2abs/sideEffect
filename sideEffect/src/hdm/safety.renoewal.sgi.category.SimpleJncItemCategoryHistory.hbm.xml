<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class table="sgi_jnc_category_history" name="safety.renewal.sgi.category.SimpleJncItemCategoryHistory"> 
<id type="long">
    <column name="id">
    </column>
    <generator class="sequence">
        <param name="sequence">
            seq_pk_jnc_category_history
        </param>
    </generator>
</id>

<property name="categoryId">
    <column name="category_id"></column>
</property>
 
<property name="historyId" >
 <column name="history_id"/>
</property>

</class> 
</hibernate-mapping>
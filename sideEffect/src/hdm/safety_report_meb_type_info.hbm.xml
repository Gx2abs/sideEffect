<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class table="safety_report_meb_type_info"  name="properties.SimpleSafetyReportMebTypeInfo"> 
	
		<id name="id">
			 <column name="id"/>
			 <generator class="sequence">
				<param name="sequence">safety_report_meb_type_info_seq</param>
			</generator> 
		</id>
		
		<property name="report_id">
			 <column name="report_id"/>
		</property>
		
		<property name="meddev_item_seq">
			 <column name="meddev_item_seq"/>
		</property>


	</class> 
 </hibernate-mapping>
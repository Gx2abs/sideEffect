<properties name="sideEffectReportDateLink" insert="false" update="false" unique="false" >
    <property 
        name="report_id" column="report_id" generated="never"
        insert="false" unique="false" update="false"
        >
    </property>
</properties>
 -->

</class> 
</hibernate-mapping>
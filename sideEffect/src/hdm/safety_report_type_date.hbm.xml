<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class table="SAFETY_REPORT_TYPE_DATE" name="safety.SimpleSafetyReportTypeDate"> 
<id name="id" column="id" >
 <generator class="sequence"> 
	  	<param name="sequence">SAFETY_REPORT_TYPE_DATE_SEQ</param> 
  </generator>
</id>
 
<property name="report_id" >
 <column name="report_id"/>
</property>
<property name="junc_id" >
 <column name="junc_id"/>
</property>
<property name="report_date" >
 <column name="report_date"/>
</property>

<property name="report_type_id" >
 <column name="report_type_id"/>
</property>

<property name="report_year" >
 <column name="report_year"/>
</property>

<property name="report_month" >
 <column name="report_month"/>
</property>

<many-to-one 
    class="statistics.property.SimpleReportDate"
    name="reportYM"
    insert="false"
    update="false"
    not-found="ignore"
    lazy="false"
    property-ref="reportDateLink"
    >
    <column name="report_year"/>
    <column name="report_month"/>
</many-to-one>


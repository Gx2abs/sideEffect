<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class table="SGI_SE_ITEM_TYPE_INFO" name="safety.renewal.sgi.item.SimpleItemType"
      dynamic-insert="true" 
     dynamic-update="true" 
     mutable="true">
<id name="id">
 <column name="id"/>
   <generator class="sequence">
				<param name="sequence">SE_SEQ_ITEM_TYPE_NO_PK</param>
   </generator> 
</id>
<property name="item_id">
 <column name="item_id"/>
</property>
<property name="type_name">
 <column name="type_name"/>
</property>
<property name="delete_yn">
 <column name="delete_yn"/>
</property>


	 
</class> 
 </hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class table="SIDEEFFECT_RESULT_HISTORY" name="sideeffect.SimpleSideeffectResultHistory" > 
	
		<id name="id" column="id" >
			<generator class="sequence">
				<param name="sequence">SIDEEFFECT_RESULT_HISTORY_SEQ</param>
			</generator> 
		</id>
		
		<property name="activeFrom">
		 	<column name="active_from"/>
		</property>
		
		<property name="historyType">
		 	<column name="history_type"/>
		</property>
		
		<property name="historyDescription">
		 	<column name="history_description"/>
		</property>
		
		<property name="manager">
		 	<column name="manager"/>
		</property>
		
		<property name="lastModified">
		 	<column name="last_modified"/>
		</property>
		
	</class> 
</hibernate-mapping>